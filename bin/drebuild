#!/bin/sh

docker-compose --project-name "tvs" down
docker-compose --project-name "tvs" rm -f -v
docker-compose --project-name "tvs" run server rake db:drop
docker-compose --project-name "tvs" run server rake db:setup
docker-compose --project-name "tvs" down
echo "Rebuild completed. You can now start up TVS"
